{
  "name": "@jedwards1211/js-toolchain",
  "version": "0.0.0-development",
  "description": "build tools and package scripts for js packages",
  "main": "index.js",
  "author": "Andy Edwards",
  "license": "MIT",
  "files": [
    "bin",
    "content",
    "util",
    ".babelrc.js",
    ".eslintrc.js",
    ".nyc.config-host.js",
    ".nyc.config.js",
    ".prettierrc.js",
    "commitlint.config.js",
    "lint-staged.config.js"
  ],
  "bin": {
    "toolchain": "./bin/toolchain.js"
  },
  "scripts": {
    "prepublishOnly": "bin/toolchain.js check && bin/toolchain.js test",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./bin/toolchain.js pre-commit",
      "commit-msg": "./bin/toolchain.js commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "mocha": "test/index.js"
  },
  "dependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-flow": "^7.10.4",
    "@babel/register": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@jedwards1211/commitlint-config": "^1.0.1",
    "@jedwards1211/eslint-config": "^2.0.0",
    "@jedwards1211/eslint-config-flow": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "codecov": "^3.8.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "mocha": "^8.1.3",
    "nyc": "^15.1.0",
    "open-cli": "^6.0.1",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.1.2"
  },
  "devDependencies": {
    "promisify-child-process": "^4.1.1"
  }
}
